<tr>
  <td class="v-top w-10 b pt2 pb3 pr3 bt b--light-gray lh-copy"> Acabamentos em Pintura </td>
  <td class="v-top w-10  pt1 pb3 pr3 bt b--light-gray pt2">
    <% allColors.filter(function(color){return color.fields['Grupo'] === 'Pintura'}).forEach(function(color){ %>
    <div class="db fl f6">
      <% if(color.fields['Render']) { %>
      <div class="br-pill overflow-hidden w1 h1 relative mr1 mt1">
        <img style="transform:scale(2)" class="absolute center tc"
          src="<%= color.fields['Render'][0]['thumbnails']['large']['url'] %>" alt="">
      </div>
      <% } %>
    </div>
    <% }); %>
  </td>
</tr>