<div id="product-content-header" class="w-100 db cf">
  <% if (data["Nome"]) {  %>
    <h1 class="fl mt0"> <%= data["Tipo"] + " " + data["Nome"] %> </h1>
  <% } else { %>
    <h1 class="fl mt0"> <%= data["Tipo"] + " " + data["Código"] %> </h1>
  <% } %>
    <% if (data["Ficha Técnica"]) {  %>
        <a class="pa3 fr-l w-auto-l w-100 tc fl dib-l db f6 ba ba-red red link bg-animate hover-bg-red hover-white pointer mb3" target='blank' href= <%= data["Ficha Técnica"][0]['url'] %> > ⤓ Ficha técnica </a>
    <% } %>
    <% if (data["Modelo 3Dmax"]) {  %>
        <a class="pa3 fr-l w-auto-l w-100 tc fl dib-l db f6 ba ba-red red link bg-animate hover-bg-red hover-white pointer mb3" target='blank' href= <%= data["Modelo 3Dmax"][0]['url'] %> > ⤓ 3Dmax </a>
    <% } %>
    <% if (data["Modelo SketchUp"]) {  %>
        <a class="pa3 fr-l w-auto-l w-100 tc fl dib-l db f6 ba ba-red red link bg-animate hover-bg-red hover-white pointer mb3" target='blank' href= <%= data["Modelo SketchUp"][0]['url'] %> > ⤓ SketchUp </a>
    <% } %>
</div>
